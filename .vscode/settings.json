{
   "editor.formatOnSave": true,
   "editor.codeActionsOnSave": {
      "source.fixAll": "always",
      "source.organizeImports": "always",
      "source.addMissingImports": "explicit"
   },
   "files.associations": {
      "*.css": "tailwindcss"
   },
   "typescript.tsdk": "node_modules/typescript/lib",
   "conventionalCommits.scopes": [
      "auth",
      "Doctor",
      "Appointment",
      "Patient",
      "BE-Integration"
   ]
}
